<?xml version="1.0" encoding="UTF-8"?>
<identify id="sample.jiazhuang" name="汽车行业" match="ddySignUser AND qicheBiz"
	priority="10">
	<matchers>
		<matcher id="ddySignUser" type="userTag" test="ddy_biz_user"></matcher>
		<matcher id="qicheBiz" type="expression" test="#bizCode == 'jiazhuang'"></matcher>
	</matchers>
	<service-configs>
		<service code="user.name.modify" version="1.0.0">
			<config id="industryEnable" value="1"></config>
			<config id="industry" value="2"></config>
		</service>
	</service-configs>
</identify>
	
